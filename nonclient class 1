public class Player
{
    private String name;
    private String team; 
    public Player ()
    {
        name = "null";
        team = "null";
    }
    public Player (String n, String t)
    {
        name = n;
        team = t;
    }
    public String getName ()
    {
        return name;
    }
    public String getTeam ()
    {
        return team;
    }
}
